  sections: Page[],
  title: string
}
const { sections, title } = Astro.props as Props
---

<nav class="bg-baby-powder p-6 rounded-md flex flex-col gap-4">
  <h4 class="text-xl">Sidans inneh√•ll</h4>
  <a class="text-russian-green underline" href="#">{title}</a>
